<%= simple_form_for course do |f| %>
<%= f.input :title %>
<%= f.input :description %>
<%= f.input :address %>
<%= f.input :begin_date %>
<%= f.input :end_date %>
<%= f.input :price %>
<%= image_tag(course.photo_url) if course.photo? %>
<%= f.input :photo %>
<%= f.hidden_field :photo_cache %>
<%= f.submit class:"btn red-button lg" %>
<% end %>
