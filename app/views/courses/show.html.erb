<div class="container">
  <div class="row">
    <div class="col-xs-12 col-md-6 col-md-offset-3">
     <%= render partial: 'shared/card', locals: {
      course: @course } %>
      <%= link_to "Edit", edit_course_path(@course), class:"btn" %>
      <%= link_to "Delete",  course_path(@course), method: :delete, class:"btn" %>
      <%= link_to "Join course", course_bookings_path(@course), method: :post,
      class: "btn btn-success" %>
    </div>
  </div>
</div>
